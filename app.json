{
  "expo": {
    "name": "fitness-tracking-app",
    "slug": "fitness-app",
    "version": "1.0.0",
    "icon": "./assets/images/splashscreen_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/images/splashscreen_logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashscreen_logo.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACTIVITY_RECOGNITION"
      ],
      "package": "com.mayadagh.fitnesstrackingapp",
      "icon": "./assets/images/splashscreen_logo.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashscreen_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#232323"
        }
      ],
      "expo-asset",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0b943bdf-1703-43f1-bbae-b4235065d7ed"
      }
    },
    "owner": "mayadaha"
  }
}
